{
  "ConnectionStrings": {
    "Default": "ADD_FROM_SECRETS"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "RequestLoggingSettings": {
    "Text": "SomeTextForLogging"
  },

  "AllowedHosts": "*",

  
    "ImageStorageSettings": {
      "ContentType": "image/jpg",
      "MaxAvatarImageFileSize": "200*200",
      "MaxProductFileSize": "1024*682",
      "MaxImageFileSizeInBytes": 1048576,
      "AllowedFileExtensions": [ ".jpg", ".jpeg", ".png", ".webp" ],
      "Jpeg": {
        "Quality": 80
      },
      "Png": {
        "CompressionLevel": 6
      },
      "Webp": {
        "Quality": 80,
        "FileFormat": "Lossy"
      }
    }
}